{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "dist",
    "rootDirs": [
      "src",
      "../lib/src"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "botlandia/*": [
        "./src/*"
      ],
      "botlandia/lib/*": [
        "../lib/src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
